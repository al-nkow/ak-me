    .int-tabs
        dev-tabs(candidate="vc.candidate")

    int-row-head(title="'Queues'" type="'mark'")
    int-row-mark(title="'RabbitMQ'" model="vc.card.rabbitMq" toggle="vc.edit")
    int-row-mark(title="'ZeroMQ'" model="vc.card.zeroMq" toggle="vc.edit")
    int-row-mark(title="'JMS'" model="vc.card.jmx" toggle="vc.edit")
    int-row-mark(title="'Kafka'" model="vc.card.kafka" toggle="vc.edit")

    int-row-head(title="'Real-Time Processing'" type="'mark'")
    int-row-mark(title="'Spark'" model="vc.card.spark" toggle="vc.edit")
    int-row-mark(title="'Storm'" model="vc.card.storm" toggle="vc.edit")
    int-row-mark(title="'Samza'" model="vc.card.samza" toggle="vc.edit")
    int-row-mark(title="'Flink'" model="vc.card.flink" toggle="vc.edit")
    int-row-mark(title="'AWS Lambda'" model="vc.card.awsLambda" toggle="vc.edit")

    int-row-head(title="'Hadoop Ecosystem'" type="'mark'")
