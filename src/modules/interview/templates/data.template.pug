    int-row-mark(title="'Flume'" model="vc.card.flume" toggle="vc.edit")
    int-row-mark(title="'Sqoop'" model="vc.card.sqoop" toggle="vc.edit")
    int-row-mark(title="'Pig'" model="vc.card.pig" toggle="vc.edit")
    int-row-mark(title="'Hive'" model="vc.card.hive" toggle="vc.edit")
    int-row-mark(title="'Oozie'" model="vc.card.oozie" toggle="vc.edit")
    int-row-mark(title="'HBase'" model="vc.card.hbase" toggle="vc.edit")
    int-row-mark(title="'Zookeeper'" model="vc.card.zookeeper" toggle="vc.edit")

    .f-buttons-wrap(layout="row" layout-align="end center")
        button.btn.btn-primary.m-btn(ng-if="vc.edit" ng-click="vc.saveCard();") SAVE